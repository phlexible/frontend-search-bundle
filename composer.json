{
    "name": "phlexible/frontend-search-bundle",
    "description": "phlexible frontend search bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["indexer", "search"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\FrontendSearchBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/elastica-bundle": "^1.2",
        "phlexible/indexer-page-bundle": "^1.3",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
